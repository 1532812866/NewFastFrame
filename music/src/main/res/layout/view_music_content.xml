<?xml version="1.0" encoding="utf-8"?>

<com.example.cootek.newfastframe.view.slide.SlidingPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sl_view_music_content_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    android:orientation="vertical"
    android:background="@color/base_color_text_white"
    app:umanoDragView="@+id/fl_view_music_content_bottom"
    app:umanoOverlay="false"
    app:umanoPanelHeight="60dp"
    app:umanoShadowHeight="5dp">

    <FrameLayout
        android:id="@+id/fl_view_music_content_bottom"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</com.example.cootek.newfastframe.view.slide.SlidingPanelLayout>